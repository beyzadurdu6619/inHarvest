
@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}


@using EntityLayer.Concreate
@model Contact

<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Mesaj Kutusu</h4>

            <form class="forms-sample" method="post">
                @Html.HiddenFor(x => x.ContactID)
                <div class="form-group">
                    <label for="exampleInputName1">Açıklama1</label>
                    <input type="text" class="form-control" id="exampleInputName1" placeholder="Başlık Giriniz." asp-for="Name" readonly>
                </div>
                <div class="form-group">
                    <label for="exampleInputName1">Açıklama2</label>
                    <input type="text" class="form-control" id="exampleInputName2" placeholder="Başlık Giriniz." asp-for="Mail" readonly>
                </div>
                <div class="form-group">
                    <label for="exampleInputName1">Açıklama3</label>
                    <input type="text" class="form-control" id="exampleInputName3" placeholder="Başlık Giriniz." asp-for="Message" readonly>
                </div>
                <div class="form-group">
                    <label for="exampleInputName1">Açıklama4</label>
                    <input type="date" class="form-control" id="exampleInputName4" placeholder="Başlık Giriniz." asp-for="Date" readonly>
                </div>
       



            </form>
        </div>
    </div>
</div>


