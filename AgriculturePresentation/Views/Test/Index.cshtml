@{
}
@using EntityLayer.Concreate
@model List<Services>
<h1>Hizmetler Listesi</h1>
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Başlık</th>


    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ServicesID</td>
            <td>@item.Title</td>

        </tr>
    }
</table>
<a href="/Test/Create" class="btn btn-outline-info">Yeni Hizmet Girişi</a>
